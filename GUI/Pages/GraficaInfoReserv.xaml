<UserControl x:Class="GUI.Pages.GraficaInfoReserv"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GUI.Pages"
             xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             d:DataContext="{d:DesignInstance Section}">
    <Grid>
        <lvc:PieChart Name="GraficaReservas" LegendLocation="Bottom" DataClick="Chart_OnDataClick" Foreground="{DynamicResource PrimaryBlueText}" Hoverable="False" DataTooltip="{x:Null}">
        </lvc:PieChart>
    </Grid>
</UserControl>
